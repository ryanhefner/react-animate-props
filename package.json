{
  "name": "react-animate-props",
  "version": "0.2.1",
  "license": "MIT",
  "description": "React higher order component method that transforms your favorite components to animate their props on change.",
  "repository": "ryanhefner/react-animate-props",
  "author": "Ryan Hefner <hi@ryanhefner.com> (https://www.ryanhefner.com)",
  "keywords": [
    "react",
    "higher-order-component",
    "react-component",
    "react-hook",
    "animate",
    "props",
    "tween",
    "tweenkle"
  ],
  "files": [
    "index.js",
    "animateProps.js",
    "createTween.js",
    "useAnimateProps.js",
    "es",
    "src",
    "umd"
  ],
  "directories": {
    "lib": "/src"
  },
  "main": "index.js",
  "module": "es/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "clean": "rm -f index.js && rm -rf animateProps.js && rm -rf createTween.js && rm -rf useAnimateProps.js && rm -rf es && rm -rf umd",
    "prebuild": "npm run clean",
    "build": "node ./tools/build.js",
    "watch": "babel ./src -d . --ignore __tests__ --watch",
    "prepare": "npm run build",
    "prepublishOnly": "node ./tools/build.js",
    "push-release": "git push origin master && git push --tags",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "peerDependencies": {
    "react": ">=15"
  },
  "dependencies": {
    "lomit": "^0.1.1",
    "react-display-name": "^0.2.4",
    "tweenkle": "^0.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-plugin-dev-expression": "^0.2.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "gzip-size": "^5.1.1",
    "jest": "^24.9.0",
    "pretty-bytes": "^5.3.0",
    "react": "^16.9.0",
    "rollup": "^1.20.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-uglify": "^6.0.2"
  }
}
